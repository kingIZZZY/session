{
    "name": "hypervel/session",
    "type": "library",
    "description": "The session package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "session",
        "swoole",
        "hypervel"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@hypervel.org"
        }
    ],
    "support": {
        "issues": "https://github.com/hypervel/components/issues",
        "source": "https://github.com/hypervel/components"
    },
    "autoload": {
        "psr-4": {
            "Hypervel\\Session\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "require": {
        "php": "^8.2",
        "ext-session": "*",
        "hyperf/context": "3.2.x-dev",
        "hyperf/collection": "3.2.x-dev",
        "hyperf/support": "3.2.x-dev",
        "hyperf/stringable": "3.2.x-dev",
        "hyperf/macroable": "3.2.x-dev",
        "hyperf/tappable": "3.2.x-dev",
        "hyperf/view-engine": "3.2.x-dev",
        "hypervel/cache": "dev-hyperf3.2",
        "hypervel/cookie": "dev-hyperf3.2",
        "hypervel/support": "dev-hyperf3.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Session\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/cache"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/cookie"
        },
        {
            "type": "github",
            "url": "https://github.com/kingIZZZY/support"
        }
    ]
}
